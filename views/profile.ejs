            <div class="flex gap-x-2 xl:gap-x-5 w-full justify-between">
              <div class="w-[48%] sm:w-1/2 flex flex-col gap-y-1">
                <label for="firstName" class="text-xs md:text-sm lg:text-xs xl:text-sm cursor-pointer">First Name</label>
                <input type="text" name="firstName" id="firstName" placeholder="Enter your First Name" class="placeholder:text-xs sm:placeholder:text-sm outline-none focus:border-lightBlue px-4 py-2 md:py-3 border border-[#EBEBEB] rounded-md caret-lightBlue">
              </div>
              <div class="w-[48%] sm:w-1/2 flex flex-col gap-y-1">
                <label for="lastName" class="text-xs md:text-sm lg:text-xs xl:text-sm cursor-pointer">Last Name</label>
                <input type="text" name="lastName" id="lastName" placeholder="Enter your Last Name"class="placeholder:text-xs sm:placeholder:text-sm outline-none focus:border-lightBlue px-4 py-2 md:py-3 border border-[#EBEBEB] rounded-md caret-lightBlue">
              </div>
            </div>
            <div class="flex flex-col gap-y-1 mt-4">
              <label for="email" class="text-xs md:text-sm lg:text-xs xl:text-sm cursor-pointer">Email</label>
              <input type="email" name="email" id="email" placeholder="Enter your Email" class="placeholder:text-xs sm:placeholder:text-sm outline-none focus:border-lightBlue px-4 py-2 md:py-3 border border-[#EBEBEB] rounded-md caret-lightBlue">
            </div>
            <div class="flex flex-col gap-y-1 mt-4">
