      <div class="pl-3 flex gap-x-2 mt-6 cursor-pointer text-[#9EA0A3]" id="otherButton">
        <img src="/Images/crypto.svg" alt="Other Icon" id="otherIcon">
        <p class="text-sm md:text-base" id="otherText">Other Information</p>
      </div>
    </div>
    <div class="lg:w-[70%]">
      <section id="profileDetails" class="">
        <div class="px-3">
          <h1 class="text-lg sm:text-xl md:text-2xl xl:text-3xl">
            Your Information
          </h1>
          <img src="/Images/profilePicture.jpeg" alt="Profile Picture" class="w-[4rem] sm:w-[5rem] md:w-[6rem] lg:w-[7rem] xl:w-[8rem] mt-4">
          <div class="flex flex-col gap-y-2 text-xs sm:text-sm xl:text-base mt-4">
            <div class="flex gap-x-1 p-2">
              <p>First Name:</p>
              <p class="text-darkBlue font-semibold"><% if (user.firstName.length === 0) {%>
                No First Name Yet
              <% } else { %>
                <%= user.firstName %>
              <% } %>
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
              <p>Last Name:</p>
              <p class="text-darkBlue font-semibold"><% if (user.lastName.length === 0) {%>
                No Last Name Yet
              <% } else { %>
                <%= user.lastName %>
              <% } %> 
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
              <p>Email:</p>
              <p class="text-darkBlue font-semibold"><% if (user.email.length === 0) {%>
                No Email Address Yet
              <% } else { %>
                <%= user.email %>
              <% } %>
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
              <p>Username:</p>
              <p class="text-darkBlue font-semibold"><% if (user.username?.length === 0) {%>
                No Username Address Yet
              <% } else { %>
                <%= user.username %>
              <% } %>
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
              <p>Verified:</p>
              <p class="text-darkBlue font-semibold capitalize">
                <%= user.isVerified %>
              </p>
            </div>
          </div>
          <div class="flex flex-col gap-y-2 text-xs sm:text-sm xl:text-base mt-4">
            <p class="font-bold text-xs sm:text-sm md:text-base xl:text-lg">More Information</p>
            <div class="flex gap-x-1 p-2">
              <p>Country:</p>
              <p class="text-darkBlue font-semibold"><% if (user.country?.length === 0) {%>
                No Country Yet
              <% } else { %>
                <%= user.country %>
              <% } %>
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
              <p>State:</p>
              <p class="text-darkBlue font-semibold"><% if (user.state?.length === 0) {%>
                No State Yet
              <% } else { %>
                <%= user.state %>
              <% } %>
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
              <p>Address:</p>
              <p class="text-darkBlue font-semibold"><% if (user.address?.length === 0) {%>
                No Address Yet
              <% } else { %>
                <%= user.address %>
              <% } %>
              </p>
            </div>
            <div class="flex gap-x-1 p-2">
